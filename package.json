        "configuration": {
            "title": "GCMP - AI Chat Models",
            "properties": {
                "gcmp.temperature": {
                    "type": "number",
                    "default": 0.1,
                    "minimum": 0,
                    "maximum": 2,
                    "description": "控制AI模型输出的随机性。较低的值（如0.1）产生更确定性的输出，较高的值（如1.0）产生更有创意的输出。",
                    "scope": "application"
                },
                "gcmp.topP": {
                    "type": "number",
                    "default": 1,
                    "minimum": 0,
                    "maximum": 1,
                    "description": "控制AI模型输出的多样性。使用较小的值（如0.9）会减少输出的随机性。",
                    "scope": "application"
                },
                "gcmp.maxTokens": {
                    "type": "number",
                    "default": 8192,
                    "minimum": 32,
                    "maximum": 32768,
                    "description": "控制AI模型输出的最大token数量。较大的值允许更长的响应。",
                    "scope": "application"
                },
                "gcmp.zhipu.search.enableMCP": {
                    "type": "boolean",
                    "default": true,
                    "description": "启用智谱AI搜索的MCP SDK模式。使用标准 MCP 协议和官方 SDK 实现搜索功能，提供更稳定的连接和更好的错误处理。非订阅套餐请关闭此开关以使用标准计费接口（按次计费）。需要有效的智谱AI API密钥。",
                    "scope": "application"
                },
                "gcmp.editToolMode": {
                    "type": "string",
                    "enum": [
                        "claude",
                        "gpt-5",
                        "none"
                    ],
                    "enumDescriptions": [
                        "使用 Claude 风格的编辑工具 (ReplaceString) - 高效精确的单次替换",
                        "使用 GPT-5 风格的编辑工具 (ApplyPatch) - 批量差异应用，支持复杂重构",
                        "不使用特定优化工具，直接文件编辑 - 效率较低，适合处理编辑异常情况"
                    ],
                    "default": "claude",
                    "description": "选择编辑工具模式。Claude 使用高效的ReplaceString，GPT-5 使用ApplyPatch进行批量编辑，None 直接使用基础文件编辑。",
                    "scope": "application"
                },
                "gcmp.providerOverrides": {
                    "type": "object",
                    "default": {},
                    "description": "供应商配置覆盖。允许覆盖供应商的baseUrl和模型配置，支持添加新模型或覆盖现有模型的参数。",
                    "scope": "application"
                },
                "gcmp.compatibleModels": {
                    "type": "array",
                    "default": [],
                    "description": "Compatible Provider 的自定义模型配置。完全由用户配置的 OpenAI / Anthropic 兼容模型列表。",
                    "scope": "application",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "模型ID",
                                "minLength": 1
                            },
                            "name": {
                                "type": "string",
                                "description": "模型显示名称",
                                "minLength": 1
                            },
                            "tooltip": {
                                "type": "string",
                                "description": "模型描述"
                            },
                            "baseUrl": {
                                "type": "string",
                                "description": "API基础URL",
                                "format": "uri"
                            },
                            "model": {
                                "type": "string",
                                "description": "API请求时使用的模型名称（可选，默认使用模型ID）"
                            },
                            "maxInputTokens": {
                                "type": "number",
                                "description": "最大输入token数量",
                                "minimum": 128
                            },
                            "maxOutputTokens": {
                                "type": "number",
                                "description": "最大输出token数量",
                                "minimum": 8
                            },
                            "sdkMode": {
                                "type": "string",
                                "enum": [
                                    "openai",
                                    "anthropic"
                                ],
                                "description": "SDK模式：openai（OpenAI兼容格式）或 anthropic（Anthropic兼容格式）",
                                "default": "openai"
                            },
                            "capabilities": {
                                "type": "object",
                                "properties": {
                                    "toolCalling": {
                                        "type": "boolean",
                                        "description": "是否支持工具调用"
                                    },
                                    "imageInput": {
                                        "type": "boolean",
                                        "description": "是否支持图像输入"
                                    }
                                },
                                "required": [
                                    "toolCalling",
                                    "imageInput"
                                ]
                            }
                        },
                        "required": [
                            "id",
                            "name",
                            "maxInputTokens",
                            "maxOutputTokens",
                            "capabilities"
                        ]
                    }
                }
            }
        },
        "jsonValidation": [
