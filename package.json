{
  "name": "gcmp",
  "publisher": "vicanent",
  "displayName": "AI Chat Models",
  "description": "为 GitHub Copilot Chat 提供 智谱AI、MoonshotAI、DeepSeek、魔搭社区、iFlow心流 模型支持的扩展",
  "version": "0.1.4",
  "license": "MIT",
  "keywords": [
    "ai",
    "chat",
    "copilot",
    "github-copilot",
    "language-model",
    "zhipu",
    "moonshot",
    "deepseek",
    "modelscope",
    "iflow",
    "智谱",
    "月之暗面",
    "魔搭",
    "心流"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/VicBilibily/gcmp.git"
  },
  "engines": {
    "node": "^20.19.0",
    "vscode": "^1.104.0"
  },
  "categories": [
    "AI",
    "Chat"
  ],
  "main": "./out/extension.js",
  "enabledApiProposals": [
    "chatProvider"
  ],
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "gcmp.zhipu.setApiKey",
        "title": "设置 智谱AI API密钥",
        "category": "GCMP"
      },
      {
        "command": "gcmp.moonshot.setApiKey",
        "title": "设置 MoonshotAI API密钥",
        "category": "GCMP"
      },
      {
        "command": "gcmp.deepseek.setApiKey",
        "title": "设置 DeepSeek API密钥",
        "category": "GCMP"
      },
      {
        "command": "gcmp.modelscope.setApiKey",
        "title": "设置 魔搭社区 API密钥",
        "category": "GCMP"
      },
      {
        "command": "gcmp.iflow.setApiKey",
        "title": "设置 iFlow API密钥",
        "category": "GCMP"
      }
    ],
    "configuration": {
      "title": "GCMP - AI Chat Models",
      "properties": {
        "gcmp.temperature": {
          "type": "number",
          "default": 0.1,
          "minimum": 0.0,
          "maximum": 2.0,
          "description": "控制AI模型输出的随机性。较低的值（如0.1）产生更确定性的输出，较高的值（如1.0）产生更有创意的输出。",
          "scope": "application"
        },
        "gcmp.topP": {
          "type": "number",
          "default": 1.0,
          "minimum": 0.0,
          "maximum": 1.0,
          "description": "控制AI模型输出的多样性。使用较小的值（如0.9）会减少输出的随机性。",
          "scope": "application"
        },
        "gcmp.maxTokens": {
          "type": "number",
          "default": 4096,
          "minimum": 1,
          "maximum": 32768,
          "description": "控制AI模型输出的最大token数量。较大的值允许更长的响应。",
          "scope": "application"
        }
      }
    },
    "languageModelChatProviders": [
      {
        "vendor": "gcmp.zhipu",
        "displayName": "[GCMP] 智谱AI",
        "managementCommand": "gcmp.zhipu.setApiKey"
      },
      {
        "vendor": "gcmp.moonshot",
        "displayName": "[GCMP] MoonshotAI",
        "managementCommand": "gcmp.moonshot.setApiKey"
      },
      {
        "vendor": "gcmp.deepseek",
        "displayName": "[GCMP] DeepSeek",
        "managementCommand": "gcmp.deepseek.setApiKey"
      },
      {
        "vendor": "gcmp.modelscope",
        "displayName": "[GCMP] 魔搭社区",
        "managementCommand": "gcmp.modelscope.setApiKey"
      },
      {
        "vendor": "gcmp.iflow",
        "displayName": "[GCMP] iFlow心流",
        "managementCommand": "gcmp.iflow.setApiKey"
      }
    ]
  },
  "scripts": {
    "clean": "rimraf out",
    "vscode:prepublish": "npm run clean && npm run esbuild-base -- --minify",
    "esbuild-base": "npm run clean && node esbuild.mjs",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "compile": "npm run clean && tsc -p ./",
    "lint": "eslint",
    "watch": "npm run clean && tsc -watch -p ./",
    "package": "vsce package",
    "publish": "vsce publish --allow-proposed-apis chatProvider",
    "pretest": "npm run compile",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.61.0",
    "@vscode/prompt-tsx": "^0.4.0-alpha.5",
    "openai": "^5.20.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@stylistic/eslint-plugin": "^2.9.0",
    "@types/node": "^22",
		"@types/vscode": "^1.104.0",
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.13.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "volta": {
    "node": "20.19.5"
  }
}
