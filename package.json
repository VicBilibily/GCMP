        "configuration": {
            "title": "GCMP - AI Chat Models",
            "properties": {
                "gcmp.temperature": {
                    "type": "number",
                    "default": 0.1,
                    "minimum": 0,
                    "maximum": 2,
                    "description": "控制AI模型输出的随机性。较低的值（如0.1）产生更确定性的输出，较高的值（如1.0）产生更有创意的输出。",
                    "scope": "application"
                },
                "gcmp.topP": {
                    "type": "number",
                    "default": 1,
                    "minimum": 0,
                    "maximum": 1,
                    "description": "控制AI模型输出的多样性。使用较小的值（如0.9）会减少输出的随机性。",
                    "scope": "application"
                },
                "gcmp.maxTokens": {
                    "type": "number",
                    "default": 8192,
                    "minimum": 32,
                    "maximum": 32768,
                    "description": "控制AI模型输出的最大token数量。较大的值允许更长的响应。",
                    "scope": "application"
                },
                "gcmp.zhipu.search.enableMCP": {
                    "type": "boolean",
                    "default": true,
                    "description": "启用智谱AI搜索的SSE通讯模式。仅 Pro+ 套餐支持通过 MCP SSE 方式调用搜索，非订阅套餐请关闭此开关以使用标准计费接口（按次计费）。需要有效的智谱AI API密钥。",
                    "scope": "application"
                },
                "gcmp.editToolMode": {
                    "type": "string",
                    "enum": [
                        "claude",
                        "gpt-5",
                        "grok-code",
                        "none"
                    ],
                    "enumDescriptions": [
                        "使用 Claude 风格的编辑工具 (ReplaceString) - 高效精确的单次替换",
                        "使用 GPT-5 风格的编辑工具 (ApplyPatch) - 批量差异应用，支持复杂重构",
                        "使用 Grok 风格的编辑工具 - 实验性设置，根据配置使用ReplaceString或EditFile",
                        "不使用特定优化工具，直接文件编辑 - 效率较低，适合处理编辑异常情况"
                    ],
                    "default": "claude",
                    "description": "选择编辑工具模式。Claude 使用高效的ReplaceString，GPT-5 使用ApplyPatch进行批量编辑，Grok 根据实验配置选择工具，None 直接使用基础文件编辑。",
                    "scope": "application"
                }
            }
        },
