{
    "name": "gcmp",
    "publisher": "vicanent",
    "displayName": "AI Chat Models",
    "description": "为 GitHub Copilot Chat 提供 智谱AI、Kimi、火山方舟、MiniMax、MoonshotAI、DeepSeek、快手万擎、阿里云百炼 模型支持的扩展，并提供 `OpenAI / Anthropic Compatible` 自定义模型支持。",
    "version": "0.14.16",
    "license": "MIT",
    "keywords": [
        "ai",
        "chat",
        "copilot",
        "github-copilot",
        "language-model",
        "zhipu",
        "kimi",
        "volcengine",
        "minimax",
        "moonshot",
        "deepseek",
        "streamlake",
        "dashscope",
        "baidu",
        "tbox",
        "OpenAI Compatible",
        "Anthropic Compatible",
        "智谱AI",
        "火山方舟",
        "月之暗面",
        "深度求索",
        "快手万擎",
        "阿里云百炼",
        "百灵大模型",
        "百度千帆",
        "Coding Plan 编程套餐"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/VicBilibily/gcmp.git"
    },
    "engines": {
        "vscode": ">=1.104.0",
        "node": ">=20.0.0",
        "npm": ">=9.0.0"
    },
    "extensionDependencies": [
        "github.copilot-chat"
    ],
    "categories": [
        "AI",
        "Chat"
    ],
    "icon": "./gcmp-logo.png",
    "main": "./dist/extension.js",
    "enabledApiProposals": [
        "chatProvider"
    ],
    "activationEvents": [
        "onStartupFinished",
        "onLanguageModelProvider:gcmp.zhipu",
        "onLanguageModelProvider:gcmp.kimi",
        "onLanguageModelProvider:gcmp.volcengine",
        "onLanguageModelProvider:gcmp.minimax",
        "onLanguageModelProvider:gcmp.iflow",
        "onLanguageModelProvider:gcmp.moonshot",
        "onLanguageModelProvider:gcmp.deepseek",
        "onLanguageModelProvider:gcmp.streamlake",
        "onLanguageModelProvider:gcmp.dashscope",
        "onLanguageModelProvider:gcmp.tbox",
        "onLanguageModelProvider:gcmp.modelscope",
        "onLanguageModelProvider:gcmp.baidu",
        "onLanguageModelProvider:gcmp.compatible",
        "onLanguageModelTool:gcmp_zhipuWebSearch",
        "onLanguageModelTool:gcmp_minimaxWebSearch",
        "onFileSystem:gcmp-settings"
    ],
    "contributes": {
        "commands": [
            {
                "command": "gcmp.zhipu.setApiKey",
                "title": "设置 智谱AI API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.zhipu.configWizard",
                "title": "智谱AI 配置向导（API密钥 & 搜索 MCP 模式）",
                "category": "GCMP"
            },
            {
                "command": "gcmp.kimi.setApiKey",
                "title": "设置 Kimi API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.volcengine.setApiKey",
                "title": "设置 火山方舟 API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.minimax.setApiKey",
                "title": "设置 MiniMax API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.minimax.setCodingPlanApiKey",
                "title": "设置 MiniMax Coding Plan 专用密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.minimax.setCodingPlanEndpoint",
                "title": "设置 MiniMax Coding Plan 接入点（国内/国际站）",
                "category": "GCMP"
            },
            {
                "command": "gcmp.minimax.configWizard",
                "title": "启动 MiniMax 配置向导",
                "category": "GCMP"
            },
            {
                "command": "gcmp.iflow.setApiKey",
                "title": "设置 心流AI API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.moonshot.setApiKey",
                "title": "设置 MoonshotAI API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.deepseek.setApiKey",
                "title": "设置 DeepSeek API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.streamlake.setApiKey",
                "title": "设置 快手万擎 API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.streamlake.configWizard",
                "title": "快手万擎 配置向导（API密钥 & 推理点ID）",
                "category": "GCMP"
            },
            {
                "command": "gcmp.dashscope.setApiKey",
                "title": "设置 阿里云百炼 API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.tbox.setApiKey",
                "title": "设置 百灵大模型 API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.modelscope.setApiKey",
                "title": "设置 ModelScope API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.baidu.setApiKey",
                "title": "设置 百度千帆 API密钥",
                "category": "GCMP"
            },
            {
                "command": "gcmp.compatible.manageModels",
                "title": "Compatible Provider 设置",
                "category": "GCMP"
            }
        ],
        "configuration": {
            "title": "GCMP - AI Chat Models",
            "properties": {
                "gcmp.temperature": {
                    "type": "number",
                    "default": 0.1,
                    "minimum": 0,
                    "maximum": 2,
                    "description": "控制AI模型输出的随机性。较低的值（如0.1）产生更确定性的输出，较高的值（如1.0）产生更有创意的输出。",
                    "scope": "application"
                },
                "gcmp.topP": {
                    "type": "number",
                    "default": 1,
                    "minimum": 0,
                    "maximum": 1,
                    "description": "控制AI模型输出的多样性。使用较小的值（如0.9）会减少输出的随机性。",
                    "scope": "application"
                },
                "gcmp.maxTokens": {
                    "type": "number",
                    "default": 8192,
                    "minimum": 32,
                    "maximum": 32768,
                    "description": "控制AI模型输出的最大token数量。较大的值允许更长的响应。",
                    "scope": "application"
                },
                "gcmp.editToolMode": {
                    "type": "string",
                    "enum": [
                        "claude",
                        "gpt-5",
                        "none"
                    ],
                    "enumDescriptions": [
                        "使用 Claude 风格的编辑工具 (ReplaceString) - 高效精确的单次替换",
                        "使用 GPT-5 风格的编辑工具 (ApplyPatch) - 批量差异应用，支持复杂重构",
                        "不使用特定优化工具，直接文件编辑 - 效率较低，适合处理编辑异常情况"
                    ],
                    "default": "claude",
                    "description": "选择编辑工具模式。Claude 使用高效的ReplaceString，GPT-5 使用ApplyPatch进行批量编辑，None 直接使用基础文件编辑。",
                    "scope": "application"
                },
                "gcmp.rememberLastModel": {
                    "type": "boolean",
                    "default": true,
                    "description": "启用后，会记录并自动恢复上次使用的扩展提供的模型。(VSCode1.106.0版本会重置为默认最优模型[eg：Claude Sonnet 4.5])",
                    "scope": "application"
                },
                "gcmp.zhipu.search.enableMCP": {
                    "type": "boolean",
                    "default": true,
                    "description": "启用智谱AI搜索的MCP SDK模式。使用标准 MCP 协议和官方 SDK 实现搜索功能，提供更稳定的连接和更好的错误处理。非订阅套餐请关闭此开关以使用标准计费接口（按次计费）。需要有效的智谱AI API密钥。",
                    "scope": "application"
                },
                "gcmp.minimax.endpoint": {
                    "type": "string",
                    "enum": [
                        "minimaxi.com",
                        "minimax.io"
                    ],
                    "default": "minimaxi.com",
                    "description": "MiniMax Coding Plan 接入站点。minimaxi.com 为国内站（推荐），minimax.io 为国际站。",
                    "scope": "application"
                },
                "gcmp.providerOverrides": {
                    "type": "object",
                    "default": {},
                    "description": "提供商配置覆盖。允许覆盖提供商的baseUrl和模型配置，支持添加新模型或覆盖现有模型的参数。",
                    "scope": "application"
                },
                "gcmp.compatibleModels": {
                    "type": "array",
                    "default": [],
                    "description": "Compatible Provider 的自定义模型配置。完全由用户配置的 OpenAI / Anthropic 兼容模型列表。",
                    "scope": "application",
                    "items": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "模型ID",
                                "minLength": 1
                            },
                            "name": {
                                "type": "string",
                                "description": "模型显示名称",
                                "minLength": 1
                            },
                            "tooltip": {
                                "type": "string",
                                "description": "模型描述"
                            },
                            "provider": {
                                "type": "string",
                                "description": "模型提供商标识符"
                            },
                            "sdkMode": {
                                "type": "string",
                                "enum": [
                                    "openai",
                                    "openai-sse",
                                    "anthropic"
                                ],
                                "enumDescriptions": [
                                    "OpenAI SDK 标准模式，使用官方 OpenAI SDK 进行请求响应处理",
                                    "OpenAI SSE 兼容模式，使用插件内实现的SSE解析逻辑进行流式响应处理",
                                    "Anthropic SDK 标准模式，使用官方 Anthropic SDK 进行请求响应处理"
                                ],
                                "description": "SDK模式默认为 openai。",
                                "default": "openai"
                            },
                            "baseUrl": {
                                "type": "string",
                                "description": "API基础URL",
                                "format": "uri"
                            },
                            "model": {
                                "type": "string",
                                "description": "API请求时使用的模型名称（可选，默认使用模型ID）"
                            },
                            "maxInputTokens": {
                                "type": "number",
                                "description": "最大输入token数量",
                                "minimum": 128
                            },
                            "maxOutputTokens": {
                                "type": "number",
                                "description": "最大输出token数量",
                                "minimum": 8
                            },
                            "outputThinking": {
                                "type": "boolean",
                                "description": "是否启用输出思考过程（如有支持）",
                                "default": true
                            },
                            "capabilities": {
                                "type": "object",
                                "properties": {
                                    "toolCalling": {
                                        "type": "boolean",
                                        "description": "是否支持工具调用"
                                    },
                                    "imageInput": {
                                        "type": "boolean",
                                        "description": "是否支持图像输入"
                                    }
                                },
                                "required": [
                                    "toolCalling",
                                    "imageInput"
                                ]
                            },
                            "customHeader": {
                                "type": "object",
                                "description": "自定义HTTP头部配置，支持 ${APIKEY} 占位符替换",
                                "additionalProperties": {
                                    "type": "string",
                                    "description": "HTTP头部值"
                                }
                            },
                            "extraBody": {
                                "type": "object",
                                "description": "额外的请求体参数，将在API请求中合并到请求体中",
                                "additionalProperties": {
                                    "description": "额外的请求体参数值"
                                }
                            }
                        },
                        "required": [
                            "id",
                            "name",
                            "maxInputTokens",
                            "maxOutputTokens",
                            "capabilities"
                        ]
                    }
                }
            }
        },
        "jsonValidation": [
            {
                "fileMatch": "settings.json",
                "url": "gcmp-settings://root/schema.json"
            }
        ],
        "languageModelChatProviders": [
            {
                "vendor": "gcmp.zhipu",
                "displayName": "⦿ 智谱AI (GLM Coding Plan)",
                "managementCommand": "gcmp.zhipu.configWizard"
            },
            {
                "vendor": "gcmp.kimi",
                "displayName": "⦿ Kimi For Coding",
                "managementCommand": "gcmp.kimi.setApiKey"
            },
            {
                "vendor": "gcmp.volcengine",
                "displayName": "⦿ 火山方舟 (Coding Plan)",
                "managementCommand": "gcmp.volcengine.setApiKey"
            },
            {
                "vendor": "gcmp.minimax",
                "displayName": "⦿ MiniMax (Coding Plan)",
                "managementCommand": "gcmp.minimax.configWizard"
            },
            {
                "vendor": "gcmp.iflow",
                "displayName": "⨭ 心流AI (EOL on 2025-12-31)",
                "managementCommand": "gcmp.iflow.setApiKey"
            },
            {
                "vendor": "gcmp.moonshot",
                "displayName": "⦿ MoonshotAI",
                "managementCommand": "gcmp.moonshot.setApiKey"
            },
            {
                "vendor": "gcmp.deepseek",
                "displayName": "⦿ DeepSeek",
                "managementCommand": "gcmp.deepseek.setApiKey"
            },
            {
                "vendor": "gcmp.streamlake",
                "displayName": "⦿ 快手万擎 (KAT-Coder)",
                "managementCommand": "gcmp.streamlake.configWizard"
            },
            {
                "vendor": "gcmp.dashscope",
                "displayName": "⦿ 阿里云百炼",
                "managementCommand": "gcmp.dashscope.setApiKey"
            },
            {
                "vendor": "gcmp.tbox",
                "displayName": "⦿ 百灵大模型",
                "managementCommand": "gcmp.tbox.setApiKey"
            },
            {
                "vendor": "gcmp.modelscope",
                "displayName": "⨭ ModelScope (EOL on 2025-12-11)",
                "managementCommand": "gcmp.modelscope.setApiKey"
            },
            {
                "vendor": "gcmp.baidu",
                "displayName": "⨮ 百度千帆 (Beta)",
                "managementCommand": "gcmp.baidu.setApiKey"
            },
            {
                "vendor": "gcmp.compatible",
                "displayName": "⦿ OpenAI / Anthropic Compatible",
                "managementCommand": "gcmp.compatible.manageModels"
            }
        ],
        "languageModelTools": [
            {
                "name": "gcmp_zhipuWebSearch",
                "displayName": "智谱AI联网搜索",
                "toolReferenceName": "zhipuWebSearch",
                "modelDescription": "使用智谱AI提供的联网搜索功能获取实时信息。支持多种搜索引擎：search_std（标准搜索，¥0.01）、search_pro（高级搜索，¥0.03）、search_pro_sogou（搜狗搜索，¥0.05）、search_pro_quark（夸克搜索，¥0.05）。注意：Pro+ 套餐可通过 MCP SSE 调用，非订阅套餐使用标准计费接口。",
                "userDescription": "通过智谱AI联网搜索获取最新信息",
                "inputSchema": {
                    "type": "object",
                    "properties": {
                        "search_query": {
                            "type": "string",
                            "description": "搜索查询字符串"
                        },
                        "search_engine": {
                            "type": "string",
                            "enum": [
                                "search_std",
                                "search_pro",
                                "search_pro_sogou",
                                "search_pro_quark"
                            ],
                            "description": "搜索引擎类型：search_std（标准，¥0.01）、search_pro（高级，¥0.03）、search_pro_sogou（搜狗，¥0.05）、search_pro_quark（夸克，¥0.05）",
                            "default": "search_std"
                        },
                        "search_intent": {
                            "type": "boolean",
                            "description": "是否进行搜索意图分析",
                            "default": false
                        },
                        "count": {
                            "type": "integer",
                            "description": "返回搜索结果数量",
                            "minimum": 1,
                            "maximum": 20,
                            "default": 10
                        },
                        "search_domain_filter": {
                            "type": "string",
                            "description": "域名过滤器，限制搜索特定网站"
                        },
                        "search_recency_filter": {
                            "type": "string",
                            "enum": [
                                "noLimit",
                                "day",
                                "week",
                                "month",
                                "year"
                            ],
                            "description": "时间过滤器：noLimit（无限制）、day（一天内）、week（一周内）、month（一月内）、year（一年内）",
                            "default": "noLimit"
                        },
                        "content_size": {
                            "type": "string",
                            "enum": [
                                "low",
                                "medium",
                                "high"
                            ],
                            "description": "内容详细程度：low（低）、medium（中）、high（高）",
                            "default": "medium"
                        }
                    },
                    "required": [
                        "search_query"
                    ]
                },
                "canBeReferencedInPrompt": true
            },
            {
                "name": "gcmp_minimaxWebSearch",
                "displayName": "MiniMax网络搜索",
                "toolReferenceName": "minimaxWebSearch",
                "modelDescription": "使用 MiniMax Coding Plan 提供的网络搜索功能获取实时信息。需要订阅 MiniMax Coding Plan 套餐并设置专用API密钥。",
                "userDescription": "通过MiniMax网络搜索获取最新信息",
                "inputSchema": {
                    "type": "object",
                    "properties": {
                        "q": {
                            "type": "string",
                            "description": "搜索查询字符串"
                        }
                    },
                    "required": [
                        "q"
                    ]
                },
                "canBeReferencedInPrompt": true
            }
        ],
        "icons": {
            "gcmp-kimi": {
                "description": "Kimi",
                "default": {
                    "fontPath": "./fonts/kimi.ttf",
                    "fontCharacter": "\\e7de"
                }
            },
            "gcmp-minimax": {
                "description": "MiniMax",
                "default": {
                    "fontPath": "./fonts/minimax.ttf",
                    "fontCharacter": "\\ea4b"
                }
            },
            "gcmp-deepseek": {
                "description": "DeepSeek",
                "default": {
                    "fontPath": "./fonts/deepseek.ttf",
                    "fontCharacter": "\\e726"
                }
            },
            "gcmp-moonshot": {
                "description": "Moonshot",
                "default": {
                    "fontPath": "./fonts/moonshot.ttf",
                    "fontCharacter": "\\e6e0"
                }
            },
            "gcmp-compatible": {
                "description": "Compatible",
                "default": {
                    "fontPath": "./fonts/compatible.ttf",
                    "fontCharacter": "\\eac8"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run clean && npm run compile",
        "compile": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --minify",
        "compile:dev": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --sourcemap",
        "clean": "rimraf dist",
        "lint": "eslint",
        "lint:fix": "eslint --fix",
        "format": "prettier --write \"src/**/*.{ts,js,json}\"",
        "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
        "watch": "esbuild ./src/extension.ts --bundle --outfile=dist/extension.js --external:vscode --format=cjs --platform=node --sourcemap --watch",
        "package": "vsce package",
        "publish": "vsce publish --allow-proposed-apis chatProvider",
        "pretest": "npm run compile:dev",
        "test": "echo \"No tests specified\" && exit 0"
    },
    "dependencies": {
        "@anthropic-ai/sdk": "^0.67.0",
        "@microsoft/tiktokenizer": "^1.0.10",
        "@modelcontextprotocol/sdk": "^1.20.2",
        "openai": "^6.7.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@stylistic/eslint-plugin": "^5.4.0",
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.104.0",
        "@types/json-schema": "^7.0.15",
        "@vscode/vsce": "^3.6.2",
        "esbuild": "^0.25.10",
        "eslint": "^9.13.0",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.39.0"
    },
    "volta": {
        "node": "20.19.5"
    }
}
